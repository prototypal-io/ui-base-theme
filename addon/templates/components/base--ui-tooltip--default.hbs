<div {{ui-root}}>
  {{yield (hash
    target=(component "ui-tooltip---target"
      kind=kind
      tooltip=tooltip
    )
    popup=(component "ui-popup"
      targetId=tooltip.targetId
      showPopup=tooltip.showTooltip
      show=tooltip.action.show
      hide=tooltip.action.hide
      toggle=tooltip.action.action.toggle
    )
  )}}
</div>
